@*onload="CargarProducto('0','0','a')"*@
<body class="loading-overlay-showing" data-loading-overlay data-plugin-options="{'hideDelay': 500}">
    <div class="loading-overlay">
        <div class="bounce-loader">
            <div class="bounce1"></div>
            <div class="bounce2"></div>
            <div class="bounce3"></div>
        </div>
    </div>

    <div class="body">

        <header id="header" class="header-full-width solid-header" data-plugin-options="{'stickyEnabled': true, 'stickyEnableOnBoxed': true, 'stickyEnableOnMobile': true, 'stickyStartAt': 78, 'stickySetTop': '0'}">
            <div class="header-body" style="background-color:black">
                <div class="header-container container">
                    <div class="header-row">
                        <div class="header-column">
                            <div class="header-row">
                                <div class="header-logo">
                                    <a href="#">
                                        <img alt="Lucy pinta tus ideas" width="120" height="60" src="~/img/products/Empresa/@ViewBag.empresa/@ViewBag.imagen2">
                                        <font color="#FF0080" face="Lucida Bright">&nbsp;&nbsp;Galeria Lucy pinta tus ideas</font>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="header-column justify-content-end">
                            <div class="header-row">
                                <div class="header-nav">
                                    <div class="header-nav-main header-nav-main-photography-effect-1 header-nav-main-sub-effect-1">
                                        <nav class="collapse">
                                            <ul class="nav nav-pills" id="mainNav">
                                                <li class="dropdown-primary">

                                                    <a href="/PrincipalPinturas"><font color="#78288C"> Inicio </font></a>

                                                </li>
                                                <li class="dropdown-primary">

                                                    <a class="nav-link  color-white" href="javascript:CargarProductoPinturas('0','0','0')">Nuevas</a>

                                                  
                                                </li>
                                                @if (ViewBag.IdProducto == null)
                                                {
                                                    @foreach (var itemC in ViewBag.Categorias)
                                                    {
                                                        <li class="dropdown dropdown-primary ">
                                                            <a class="nav-link dropdown-toggle color-white">
                                                                @itemC.categoria
                                                            </a>
                                                            <ul class="dropdown-menu">
                                                                @*<li>
                            <a class="dropdown-item" href="demo-photography-2-about-me.html">
                                About Me
                            </a>
                        </li>
                        <li>
                            <a class="dropdown-item" href="demo-photography-2-about-us.html">
                                About Us
                            </a>
                        </li>*@

                                                                @foreach (var itemS in ViewBag.subCategorias)
                                                                {@if (@itemC.idCategoria == @itemS.idCategoria)
                                                                    {

                                                                        <li><a class="dropdown-item" href="javascript:CargarProductoPinturas('@itemC.idCategoria','@itemS.idSubCategoria','0')" @*data-thumb-preview="/img/products/empresa/CYS/@itemS.imagen"*@>@itemS.subCategoria</a></li>

                                                                    }
                                                            }



                                                            </ul>
                                                        </li>
                                                    }
                                                }

                                                <li class="dropdown dropdown-primary">
                                                    <a class="nav-link dropdown-toggle active" href="#">
                                                        Contactanos
                                                    </a>
                                                    <ul class="dropdown-menu">
                                                        <li>
                                                            <a class="dropdown-item" href="#">
                                                                Correo: vazquezlucia500@gmail.com
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <a class="dropdown-item" href="#">
                                                                Teléfono: 3330318563
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <a class="dropdown-item" href="#">
                                                                Facebook:
                                                            </a>
                                                        </li>

                                                    </ul>
</nav>
                                    </div>
                                    <ul class="header-social-icons social-icons d-none d-sm-block">
                                        <li class="social-icons-facebook"><a href="http://www.facebook.com/" target="_blank" title="Facebook"><i class="fab fa-facebook-f"></i></a></li>
                                        <li class="social-icons-instagram"><a href="http://www.instagram.com/" target="_blank" title="Instagram"><i class="fab fa-instagram"></i></a></li>
                                    </ul>
                                    <button class="btn header-btn-collapse-nav" data-toggle="collapse" data-target=".header-nav-main nav">
                                        <i class="fas fa-bars"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
